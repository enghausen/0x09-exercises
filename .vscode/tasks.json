{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "build",
      "command": "g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "main.cpp",
        "datetime.cpp",
        "activity.cpp",
        "central.cpp",
        "logger.cpp",
        "-o",
        "${workspaceFolder}/build/tele-central"
      ],
      "options": {
        "cwd": "${workspaceFolder}/src"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": "build",
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}